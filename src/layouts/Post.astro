---
import Authors from "../components/Authors.astro";
import Layout from "./Layout.astro";

export interface Props {
  title: string;
}

const { title, frontmatter } = Astro.props;
---

<Layout title={title}>
  <slot />
  <Authors authors={frontmatter.authors} />
</Layout>

<style>
  img {
    max-height: 3em;
  }
</style>
